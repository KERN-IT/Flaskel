# -*- coding: utf-8 -*-
from setuptools import setup, find_packages

setup(
	name="${package}",
	version="${version}",
	description="${description}",
	author="${author}",
	author_email="${author_email}",
	url="${url}",
	install_requires=[
		"Flask==0.9",
		"Flask-SQLAlchemy==0.16",
		"SQLAlchemy==0.7.9",
		"Flask-Mail==0.7.3",
		"Flask-Script==0.5.1",
	],
	scripts=["manage.py"],
	zip_safe=False,
	packages=find_packages(),
	include_package_data=True
)
